<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <TextView
        android:id="@+id/listFechaAux"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Fecha"/>

    <TextView
        android:id="@+id/listFecha"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/listFechaAux"
        android:textColor="@color/colorPrimary"/>

    <TextView
        android:id="@+id/listTemperaturaAux"
        android:layout_below="@id/listFechaAux"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Temperatura"/>

    <TextView
        android:id="@+id/listTemperatura"
        android:layout_below="@id/listFechaAux"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/listTemperaturaAux"
        android:textColor="@color/colorPrimary"/>

    <TextView
        android:id="@+id/listHumedadAux"
        android:layout_below="@id/listFechaAux"
        android:layout_toRightOf="@id/listTemperatura"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/Humedad"/>

    <TextView
        android:id="@+id/listHumedad"
        android:layout_below="@id/listFechaAux"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_toRightOf="@id/listHumedadAux"
        android:textColor="@color/colorAccent"/>


</RelativeLayout>